# Proxy Port
PROXY_PORT=80

# Proxy SSL Port (empty if unused)
PROXY_PORT_SSL=

# Use Letsencrypt
LETSENCRYPT=0

# Version of ELK-Stack to use
ELK_VERSION=5.2.2

# Elasticsearch password for user elastic (dont't use numbers only)
ELASTIC_PASSWORD=PW123456

# Automatically pull newer docker images on startup
AUTOPULL=1

# Check ELK License and warn someone by mail if it is expiring
LICENSE_EMAIL=technik@orangehive.de
LICENSE_WARN_DAYS=60

# Use production settings
PRODUCTION=0

# Base Domain
BASE_DOMAIN=local.orangehive.de

